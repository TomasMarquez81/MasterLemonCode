{
    "compilerOptions": {
        "esModuleInterop": true,
        "isolatedModules": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "module": "esnext",
        "moduleResolution": "bundler",
        "noEmit": true,
        "jsx": "react-jsx",
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "target": "esnext",
        "useDefineForClassFields": true,
    },
    "include": [
        "src"
    ]
}